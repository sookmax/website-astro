---
type Props = {
  at: string;
  atURL: string;
};

const { at, atURL } = Astro.props;
---

<a
  class="font-semibold text-nowrap text-primary-base dark:text-primary-light hover:text-primary-dark dark:hover:text-primary-extralight"
  title={at}
  href={atURL}
  target="_blank"
  rel="noopener noreferrer"
>
  <span class="-space-x-1">
    <span>@</span>
    <span>{at}</span>
  </span>
</a>
